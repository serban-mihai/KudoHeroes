
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /getUsers                   controllers.UserController.getUsers

GET     /allUsers                   controllers.UserController.findAllUser()

# GET     /top5                   controllers.UserController.topFive

# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message
# Map static resources from the /public folder to the /assets URL path
GET     /public/*file               controllers.Assets.versioned(path="/public", file)
# Create User (Test)
POST    /new                        controllers.UserController.createUser
# Get User By ID (Test)
GET     /home/:id                   controllers.UserController.findById(id: String)
# Delete User (Test)
DELETE  /home/:id                   controllers.UserController.delete(id: String)
# Update user (Test)
PUT     /home                       controllers.UserController.update
# HOME
# GET     /home                       controllers.HomeController.index

# TESTING USER ROUTES
GET     /:id                        controllers.HomeController.index(id: String)






